{
  "info": {
    "name": "AgendaFacil API",
    "description": "API collection for AgendaFacil SaaS",
    "version": "1.0.0"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/dev",
      "type": "string"
    },
    {
      "key": "authToken",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"professional@example.com\",\n  \"password\": \"Password123!\",\n  \"name\": \"João Silva\",\n  \"phone\": \"+5511999999999\",\n  \"businessName\": \"Barbearia do João\",\n  \"businessType\": \"barbeiro\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"professional@example.com\",\n  \"password\": \"Password123!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('authToken', response.data.tokens.AccessToken);",
                  "}"
                ]
              }
            }
          ]
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/auth/me",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "me"]
            }
          }
        }
      ]
    },
    {
      "name": "Services",
      "item": [
        {
          "name": "Get Services",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/services",
              "host": ["{{baseUrl}}"],
              "path": ["services"]
            }
          }
        },
        {
          "name": "Create Service",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Corte Masculino\",\n  \"description\": \"Corte tradicional masculino\",\n  \"duration\": 30,\n  \"price\": 25.00,\n  \"category\": \"corte\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/services",
              "host": ["{{baseUrl}}"],
              "path": ["services"]
            }
          }
        },
        {
          "name": "Update Service",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Corte Masculino Premium\",\n  \"price\": 30.00\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/services/{{serviceId}}",
              "host": ["{{baseUrl}}"],
              "path": ["services", "{{serviceId}}"]
            }
          }
        },
        {
          "name": "Delete Service",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/services/{{serviceId}}",
              "host": ["{{baseUrl}}"],
              "path": ["services", "{{serviceId}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Appointments",
      "item": [
        {
          "name": "Get Appointments",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/appointments?date=2024-02-15",
              "host": ["{{baseUrl}}"],
              "path": ["appointments"],
              "query": [
                {
                  "key": "date",
                  "value": "2024-02-15"
                }
              ]
            }
          }
        },
        {
          "name": "Create Appointment",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"clientName\": \"Maria Silva\",\n  \"clientPhone\": \"+5511888888888\",\n  \"clientEmail\": \"maria@example.com\",\n  \"serviceId\": \"service-uuid\",\n  \"serviceName\": \"Corte Masculino\",\n  \"servicePrice\": 25.00,\n  \"appointmentDate\": \"2024-02-15\",\n  \"appointmentTime\": \"14:30\",\n  \"notes\": \"Cliente preferencial\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/appointments",
              "host": ["{{baseUrl}}"],
              "path": ["appointments"]
            }
          }
        },
        {
          "name": "Update Appointment",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"confirmed\",\n  \"paymentStatus\": \"paid\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/appointments/{{appointmentId}}",
              "host": ["{{baseUrl}}"],
              "path": ["appointments", "{{appointmentId}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Public Booking",
      "item": [
        {
          "name": "Get Professional by Link",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/booking/professional?link=joao-barbeiro",
              "host": ["{{baseUrl}}"],
              "path": ["booking", "professional"],
              "query": [
                {
                  "key": "link",
                  "value": "joao-barbeiro"
                }
              ]
            }
          }
        },
        {
          "name": "Get Public Services",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/booking/services?userId=user-uuid",
              "host": ["{{baseUrl}}"],
              "path": ["booking", "services"],
              "query": [
                {
                  "key": "userId",
                  "value": "user-uuid"
                }
              ]
            }
          }
        },
        {
          "name": "Get Availability",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/booking/availability?userId=user-uuid&date=2024-02-15",
              "host": ["{{baseUrl}}"],
              "path": ["booking", "availability"],
              "query": [
                {
                  "key": "userId",
                  "value": "user-uuid"
                },
                {
                  "key": "date",
                  "value": "2024-02-15"
                }
              ]
            }
          }
        },
        {
          "name": "Create Public Appointment",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"userId\": \"user-uuid\",\n  \"clientName\": \"Cliente Público\",\n  \"clientPhone\": \"+5511777777777\",\n  \"clientEmail\": \"cliente@example.com\",\n  \"serviceId\": \"service-uuid\",\n  \"appointmentDate\": \"2024-02-15\",\n  \"appointmentTime\": \"15:00\",\n  \"notes\": \"Agendamento via link público\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/booking/appointment",
              "host": ["{{baseUrl}}"],
              "path": ["booking", "appointment"]
            }
          }
        }
      ]
    }
  ]
}