{
  "version": "1",
  "cognitoConfig": {
    "identityPoolName": "agendemaisIdentityPool",
    "allowUnauthenticatedIdentities": false,
    "resourceNameTruncated": "agendexxx",
    "userPoolName": "agendemaisUserPool",
    "autoVerifiedAttributes": ["email"],
    "mfaConfiguration": "OFF",
    "mfaTypes": ["SMS Text Message"],
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "AGENDEMAIS: Código de verificação",
    "emailVerificationMessage": "Seu código de verificação é: {####}",
    "defaultPasswordPolicy": true,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [
      "Requires Lowercase",
      "Requires Uppercase",
      "Requires Numbers",
      "Requires Symbols"
    ],
    "requiredAttributes": ["email", "name"],
    "aliasAttributes": [],
    "userpoolClientGenerateSecret": false,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": ["email", "name", "picture"],
    "userpoolClientReadAttributes": ["email", "name", "picture", "updated_at"],
    "userpoolClientLambdaRole": "agendexxx_userpoolclient_lambda_role",
    "userpoolClientSetAttributes": true,
    "sharedId": "agendexxx",
    "resourceName": "agendemais",
    "authSelections": "identityPoolAndUserPool",
    "serviceName": "Cognito",
    "usernameAttributes": ["email"],
    "useDefault": "manual",
    "userPoolGroups": true,
    "userPoolGroupList": ["Admin", "Staff", "Clients"],
    "adminQueries": true,
    "thirdPartyAuth": false,
    "authProviders": [],
    "usernameCaseSensitive": false,
    "useEnabledMfas": true,
    "authRoleArn": {
      "Fn::GetAtt": ["AuthRole", "Arn"]
    },
    "unauthRoleArn": {
      "Fn::GetAtt": ["UnauthRole", "Arn"]
    },
    "breakCircularDependency": true,
    "dependsOn": [],
    "hostedUI": true,
    "hostedUIDomainName": "agendemais",
    "authProvidersUserPool": ["Google", "Facebook"],
    "hostedUIProviderMeta": "[{\"ProviderName\":\"Google\",\"authorize_scopes\":\"openid email profile\",\"AttributeMapping\":{\"email\":\"email\",\"name\":\"name\",\"username\":\"sub\"}},{\"ProviderName\":\"Facebook\",\"authorize_scopes\":\"public_profile,email\",\"AttributeMapping\":{\"email\":\"email\",\"name\":\"name\",\"username\":\"id\"}}]",
    "oAuthMetadata": "{\"AllowedOAuthFlows\":[\"code\"],\"AllowedOAuthScopes\":[\"phone\",\"email\",\"openid\",\"profile\",\"aws.cognito.signin.user.admin\"],\"CallbackURLs\":[\"agendemais://callback/\",\"https://agendemais.com/callback/\"],\"LogoutURLs\":[\"agendemais://signout/\",\"https://agendemais.com/signout/\"]}"
  }
}